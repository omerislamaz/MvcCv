
@{
    ViewBag.Title = "AdminEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Yeni Kullanıcı Ekleme Sayfası</h4>
<br />


<form method="post">

    <input type="text" name="KullaniciAdi" class="form-control" placeholder="Kullanıcı Adı Yazınız" />
    <br />

    <input type="text" name="Sifre" class="form-control" placeholder="Şifre Yazınız" />
    <br />


    <button class="btn btn-info">
        <i class="fa fa-plus" aria-hidden="true"></i>
        Kaydet
    </button>

    <a href="/Admin/Index/" class="btn btn-outline-info" style="float:right">
        <i class="fa fa-undo" aria-hidden="true"></i>
        Admin Sayfasına Geri Dön
    </a>

</form>



