
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />


<form method="post">

    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı Yazınız" />
    <br />

    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık Yazınız" />
    <br />

    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi Yazınız" />
    <br />

    <textarea placeholder="Açıklamanızı Giriniz" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />

    <button class="btn btn-info">
        <i class="fa fa-plus" aria-hidden="true"></i>
        Kaydet
    </button>

    <a href="/Deneyim/Index/" class="btn btn-outline-info" style="float:right">
        <i class="fa fa-undo" aria-hidden="true"></i>
        Deneyim Sayfasına Geri Dön
    </a>

</form>

